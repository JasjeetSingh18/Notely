{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "^/((?:.+)\\.(?:js|css|map|png|jpg|jpeg|gif|svg|ico|json|txt|wasm|webmanifest))$", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/$1" },
    { "src": "^(.*)$", "dest": "/index.html" }
  ]
}
